{
  "name": "jasper-ui",
  "version": "1.2.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production --source-map",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "e2e": "ng e2e",
    "cy:open": "(cd cypress; docker compose pull && docker compose up --build -d) && cypress open --config baseUrl=http://localhost:8080 && (cd cypress; docker compose down)",
    "cy:ci": "docker compose --profile ci -f 'cypress/docker-compose.yaml' up --build --exit-code-from cypress"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.6",
    "@angular/cdk": "^17.0.3",
    "@angular/common": "^17.0.6",
    "@angular/compiler": "^17.0.6",
    "@angular/core": "^17.0.6",
    "@angular/forms": "^17.0.6",
    "@angular/localize": "^17.0.6",
    "@angular/platform-browser": "^17.0.6",
    "@angular/platform-browser-dynamic": "^17.0.6",
    "@angular/router": "^17.0.6",
    "@ngx-formly/core": "^6.2.2",
    "@stomp/rx-stomp": "^2.0.0",
    "angular-oauth2-oidc": "^15.0.1",
    "chess.js": "^1.0.0-beta.6",
    "css-flatten": "^2.0.0",
    "d3": "^7.8.5",
    "europa": "^6.0.0",
    "europa-core": "^6.0.0",
    "file-saver": "^2.0.5",
    "hammerjs": "^2.0.8",
    "handlebars": "^4.7.8",
    "he": "^1.2.0",
    "hls.js": "^1.4.13",
    "jsqr": "^1.4.0",
    "jszip": "^3.10.1",
    "jtd": "^0.1.1",
    "lodash-es": "^4.17.21",
    "mobx": "^6.12.0",
    "mobx-angular": "^4.7.2",
    "moment": "^2.29.4",
    "monaco-editor": "^0.45.0",
    "ngx-markdown": "^17.1.1",
    "ngx-monaco-editor": "^12.0.0",
    "qrcode": "^1.5.3",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.0",
    "uuid": "^9.0.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.19.3/xlsx-0.19.3.tgz",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.6",
    "@angular/cli": "^17.0.6",
    "@angular/compiler-cli": "^17.0.6",
    "@cypress/schematic": "^2.5.0",
    "@ngx-formly/schematics": "^6.2.2",
    "@types/chess.js": "^0.13.7",
    "@types/d3": "^7.4.3",
    "@types/file-saver": "^2.0.7",
    "@types/hammerjs": "^2.0.45",
    "@types/he": "^1.2.3",
    "@types/jasmine": "~4.3.1",
    "@types/lodash-es": "^4.17.12",
    "@types/qrcode": "^1.5.5",
    "@types/uuid": "^9.0.7",
    "cypress": "^13.6.1",
    "cypress-mochawesome-reporter": "^3.7.0",
    "cypress-real-events": "^1.11.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "karma-junit-reporter": "^2.0.1",
    "typescript": "~5.2.2"
  },
  "overrides": {
    "ngx-monaco-editor": {
      "@angular/common": "$@angular/common",
      "@angular/core": "$@angular/core",
      "monaco-editor": "$monaco-editor"
    },
    "mobx-angular": {
      "@angular/common": "$@angular/common",
      "@angular/core": "$@angular/core",
      "@angular/cdk": "$@angular/cdk"
    },
    "dagre-d3": {
      "d3": "$d3"
    },
    "cypress-real-events": {
      "cypress": "$cypress"
    }
  },
  "browser": {
    "fs": false,
    "os": false,
    "path": false
  }
}
