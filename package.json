{
  "name": "jasper-ui",
  "version": "1.3.0",
  "scripts": {
    "postinstall": "patch-package",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production --source-map --localize",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "e2e": "npx playwright test",
    "pw:open": "(cd e2e; docker compose pull && docker compose up --build -d) && npx playwright test --ui && (cd e2e; docker compose down)",
    "pw:ci": "docker compose --profile ci -f 'e2e/docker-compose.yaml' up --build --exit-code-from playwright"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.1.2",
    "@angular/cdk": "^21.1.2",
    "@angular/common": "^21.1.2",
    "@angular/compiler": "^21.1.2",
    "@angular/core": "^21.1.2",
    "@angular/forms": "^21.1.2",
    "@angular/localize": "^21.1.2",
    "@angular/platform-browser": "^21.1.2",
    "@angular/platform-browser-dynamic": "^21.1.2",
    "@angular/router": "^21.1.2",
    "@angular/service-worker": "^21.1.2",
    "@ngx-formly/core": "7.0.0",
    "@stomp/rx-stomp": "^2.2.0",
    "array.prototype.at": "^1.1.3",
    "chess.js": "^1.4.0",
    "clipboard": "^2.0.11",
    "css-flatten": "^2.0.1",
    "d3": "^7.9.0",
    "europa": "^6.0.0",
    "europa-core": "^6.0.0",
    "file-saver": "^2.0.5",
    "globalthis": "^1.0.4",
    "hammerjs": "^2.0.8",
    "handlebars": "^4.7.8",
    "he": "^1.2.0",
    "hls.js": "^1.6.15",
    "jsqr": "^1.4.0",
    "jszip": "^3.10.1",
    "jtd": "^0.1.1",
    "katex": "^0.16.28",
    "lodash-es": "^4.17.23",
    "luxon": "^3.7.2",
    "marked": "^17.0.1",
    "mermaid": "^11.12.2",
    "monaco-editor": "^0.55.1",
    "ngx-markdown": "^21.1.0",
    "ngx-monaco-editor": "^12.0.0",
    "node-diff3": "3.1.2",
    "qrcode": "^1.5.4",
    "rxjs": "^7.4.0",
    "tslib": "^2.8.1",
    "uuid": "^13.0.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  },
  "devDependencies": {
    "@angular/build": "^21.1.2",
    "@angular/cli": "^21.1.2",
    "@angular/compiler-cli": "^21.1.2",
    "@cypress/schematic": "^5.0.0",
    "@ngx-formly/schematics": "7.0.0",
    "@playwright/test": "^1.58.2",
    "@types/chess.js": "^0.13.7",
    "@types/d3": "^7.4.3",
    "@types/file-saver": "^2.0.7",
    "@types/hammerjs": "^2.0.46",
    "@types/he": "^1.2.3",
    "@types/lodash-es": "^4.17.12",
    "@types/luxon": "^3.7.1",
    "@types/qrcode": "^1.5.6",
    "@vitest/ui": "^4.0.8",
    "jsdom": "^28.0.0",
    "patch-package": "^8.0.1",
    "raw-loader": "^4.0.2",
    "typescript": "~5.9.3"
  },
  "overrides": {
    "@angular/common": "$@angular/common",
    "@angular/core": "$@angular/core",
    "@angular/cdk": "$@angular/cdk",
    "@angular/platform-browser": "$@angular/platform-browser",
    "marked": "$marked",
    "mermaid": "$mermaid",
    "uuid": "$uuid",
    "rxjs": "$rxjs",
    "monaco-editor": "$monaco-editor",
    "d3": "$d3"
  },
  "browser": {
    "fs": false,
    "os": false,
    "path": false
  }
}
