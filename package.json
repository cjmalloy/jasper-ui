{
  "name": "jasper-ui",
  "version": "1.2.0",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production --source-map",
    "watch": "ng build --watch --configuration development",
    "electron": "electron",
    "electron:tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on tcp:4200 && npm run electron:tsc && electron . --serve",
    "electron:local": "npm run build:prod && npm run electron:tsc && electron .",
    "electron:build": "npm run build:prod && npm run electron:tsc && electron-builder build --publish=never",
    "test": "ng test"
  },
  "main": "app/main.js",
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.1.1",
    "@angular/cdk": "^14.1.1",
    "@angular/common": "^14.1.1",
    "@angular/compiler": "^14.1.1",
    "@angular/core": "^14.1.1",
    "@angular/forms": "^14.1.1",
    "@angular/platform-browser": "^14.1.1",
    "@angular/platform-browser-dynamic": "^14.1.1",
    "@angular/router": "^14.1.1",
    "angularx-qrcode": "^14.0.0",
    "d3": "^7.6.1",
    "jtd": "^0.1.1",
    "lodash-es": "^4.17.21",
    "mobx": "^6.6.1",
    "mobx-angular": "^4.6.0",
    "moment": "^2.29.4",
    "monaco-editor": "^0.34.0",
    "ngx-markdown": "^14.0.1",
    "ngx-monaco-editor": "^12.0.0",
    "rxjs": "~7.5.6",
    "tslib": "^2.3.0",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.1",
    "@angular/cli": "^14.1.1",
    "@angular/compiler-cli": "^14.1.1",
    "@types/d3": "^7.1.0",
    "@types/jasmine": "~4.0.3",
    "@types/lodash-es": "^4.17.6",
    "@types/uuid": "^8.3.4",
    "electron": "^20.0.2",
    "electron-builder": "^23.3.3",
    "electron-debug": "^3.2.0",
    "electron-reloader": "^1.2.3",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "karma-junit-reporter": "^2.0.1",
    "typescript": "~4.6.2",
    "wait-on": "^6.0.1"
  },
  "overrides": {
    "ngx-monaco-editor": {
      "@angular/common": "$@angular/common",
      "@angular/core": "$@angular/core",
      "monaco-editor": "$monaco-editor"
    }
  }
}
